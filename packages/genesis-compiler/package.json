{
    "name": "@fmfe/genesis-compiler",
    "version": "2.4.8",
    "description": "Vue SSR Microfront",
    "bin": {
        "genesis-ts-node": "./bin/ts-node.js",
        "genesis-tsc": "./bin/tsc.js"
    },
    "keywords": [
        "vue",
        "ssr",
        "Microfront"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./bin/tsc.js": "./bin/tsc.js",
        "./bin/ts-node.js": "./bin/ts-node.js",
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "scripts": {
        "build:cjs": "NODE_ENV=production genesis-tsc --build tsconfig.cjs.json",
        "build:esm": "NODE_ENV=production genesis-tsc --build tsconfig.esm.json",
        "build": "rm -rf dist types && npm run build:cjs && npm run build:esm"
    },
    "author": "Followme",
    "license": "MIT",
    "gitHead": "6a1678904351e5faadc693766042a010d600e8b7",
    "resolutions": {
        "*/vue-template-compiler": "2.7.14"
    },
    "dependencies": {
        "@babel/core": "7.19.1",
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/plugin-proposal-decorators": "7.19.1",
        "@babel/plugin-proposal-export-default-from": "7.18.10",
        "@babel/plugin-syntax-jsx": "7.18.6",
        "@babel/plugin-transform-modules-commonjs": "7.18.6",
        "@babel/plugin-transform-runtime": "7.19.1",
        "@babel/preset-env": "7.19.1",
        "@babel/runtime": "7.19.0",
        "@types/node": "18.7.18",
        "babel-helper-vue-jsx-merge-props": "2.0.3",
        "babel-loader": "8.2.5",
        "babel-plugin-syntax-jsx": "6.18.0",
        "babel-plugin-transform-vue-jsx": "3.7.0",
        "babel-preset-typescript-vue": "1.1.1",
        "core-js": "3.25.2",
        "css-loader": "6.7.1",
        "cssnano": "5.1.13",
        "esbuild-loader": "2.20.0",
        "fflate": "0.7.3",
        "file-loader": "6.2.0",
        "find": "0.3.0",
        "html-minifier": "4.0.0",
        "less": "4.1.3",
        "less-loader": "11.0.0",
        "mini-css-extract-plugin": "2.6.1",
        "mkdirp": "1.0.4",
        "postcss": "8.4.16",
        "postcss-loader": "7.0.1",
        "postcss-preset-env": "7.8.2",
        "relative": "3.0.2",
        "sass-loader": "13.0.2",
        "style-loader": "3.3.1",
        "style-resources-loader": "^1.5.0",
        "thread-loader": "3.0.4",
        "ts-node": "10.9.1",
        "typescript": "4.9.3",
        "upath": "2.0.1",
        "vue-loader": "^15.10.0",
        "vue-server-renderer": "2.7.14",
        "vue-style-loader": "4.1.3",
        "webpack": "5.74.0",
        "webpack-bundle-analyzer": "4.6.1",
        "webpack-chain": "6.5.1",
        "webpack-dev-middleware": "5.3.3",
        "webpack-hot-middleware": "2.25.2",
        "webpackbar": "5.0.2",
        "worker-loader": "3.0.8",
        "write": "2.0.0"
    },
    "devDependencies": {
        "@types/find": "0.2.1"
    },
    "homepage": "https://fmfe.github.io/genesis-docs/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fmfe/genesis.git"
    },
    "bugs": {
        "url": "https://github.com/fmfe/genesis/issues"
    }
}
